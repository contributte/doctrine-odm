{
  "name": "nettrine/odm",
  "description": "Doctrine ODM for Nette Framework",
  "keywords": ["nette", "doctrine", "odm", "database", "document"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/nettrine/odm",
  "authors": [
    {
      "name": "David Å olc",
      "homepage": "https://solc.dev"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-mongodb": "*",
    "contributte/di": "^0.5",
    "doctrine/cache": "^1.10",
    "doctrine/mongodb-odm": "^2.0",
    "doctrine/persistence": "^2.1",
    "mongodb/mongodb": "^1.5",
    "nette/di": "^3.0",
    "nette/schema": "^1.1",
    "nettrine/annotations": "^0.7 || ^0.8",
    "nettrine/cache": "^0.3 || ^0.4"
  },
  "require-dev": {
    "ninjify/qa": "^0.13",
    "ninjify/nunjuck": "^0.4.0",
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "phpstan/phpstan-nette": "^1.2",
    "phpstan/phpstan-strict-rules": "^1.4",
    "nettrine/mongodb": "^0.1.0",
    "nette/tester": "^2.3.4"
  },
  "suggest": {
    "symfony/console": "Required to use Symfony Console commands."
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\ODM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
